{% extends '::base.html.twig' %}

{% block title %}Contacto - Txikitwo{% endblock %}
{% block stylesheets %}
    {% stylesheets '@WebBundle/Resources/public/css/homepage.css'
        '@WebBundle/Resources/public/css/frontend.css'
        '@WebBundle/Resources/public/css/informacion.css'
        output='css/contacto.css' %}
    <link href="{{ asset_url }}" media="screen" rel="stylesheet" type="text/css" />
    {% endstylesheets %}
{% endblock %}

{% block body %}
    {% include ':includes:header.html.twig' %}
    {% include ':includes:menufooter.html.twig' %}
    <div id="contenido">
        <div id="texto">
            <p>Txikitwo cuenta un sistema de atención al cliente.</p>
            <p>Para más información: <strong>contacto@txikitwo.com</strong></p>
        </div>
    </div>
    {% include ':includes:footer.html.twig' %}
{% endblock %}